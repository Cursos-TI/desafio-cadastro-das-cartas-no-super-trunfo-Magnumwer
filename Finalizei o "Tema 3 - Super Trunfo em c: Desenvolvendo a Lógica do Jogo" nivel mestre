        //logica


        if (escolhaJogador2 == 2) {
            soma1 = populacao1 + area1;
            soma2 = populacao2 + area2;

            //Resultado

            printf(" O jogador 1 escolheu: %s.\n O Jogador 2 escolheu: %s\n", nomeDapais1, nomeDapais2);
            printf(" O atributo escolhido do Jogador 1: \n População: %lu\n Área: %.2f\n", populacao1, area1);
            printf(" O atributo escolhido do Jogador 2: \n População: %lu\n Área: %.2f\n", populacao2, area2);
            printf(" A soma do jogador 1: %.2f\n A soma do jogador 2: %.2f\n", soma1, soma2);

            //vencedor

            printf(" Resultado: %s\n",
            (soma1 > soma2) ? "Carta 1 venceu" :
            (soma2 > soma1) ? "Carta 2 venceu" :
            "Empate");

            //Logica

        } else if (escolhaJogador2 == 3) {
            soma1 = populacao1 + pib1;
            soma2 = populacao2 + pib2;

